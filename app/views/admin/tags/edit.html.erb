<h1>Admin::Tags#edit</h1>
<p>Find me in app/views/admin/tags/edit.html.erb</p>


<%= form_with model:@tag,local:true, url: admin_tag_path do |f| %>
  <div class='create-foem'>
    <%= f.label :タグ名,class: "control-label" %>
    <%= f.text_field :tag_name %>
    <%= f.label :ジャンル,class: "control-label" %>
    <%= f.collection_select(:genre_id, @genres, :id, :name) %>
    <%= f.submit '変更を保存' , class: 'btn btn-success' %>
  </div>
<% end %>